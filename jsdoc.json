{
  "source": {
    "include": ["src", "package.json", "README.md"],
    "includePattern": ".ts$"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "useLongnameInNav": false,
    "showInheritedInNav": true
  },
  "opts": {
    "destination": "./docs/html/",
    "encoding": "utf8",
    "private": false,
    "recurse": true,
    "template": "./node_modules/minami"
  }
}
